      "schema": "./toolchain-eslint/schema.json"
    },
    "toolchain-husky": {
      "description": "enable toolchain husky support",
      "factory": "./toolchain-husky/index#toolchainHusky"
    },
    "toolchain-lerna": {
      "description": "A blank schematic.",
